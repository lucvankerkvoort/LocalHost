{
  "name": "localhost-init",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "test": "tsx --test $(find src -name '*.test.ts' -print)",
    "test:chunk:globe-slice": "tsx --test src/store/globe-slice.test.ts",
    "test:chunk:hosts-slice": "tsx --test src/store/hosts-slice.test.ts",
    "test:chunk:host-creation-slice": "tsx --test src/store/host-creation-slice.test.ts",
    "test:chunk:utils": "tsx --test src/lib/utils.test.ts",
    "test:chunk:tool-calls-slice": "tsx --test src/store/tool-calls-slice.test.ts",
    "test:chunk:type-helpers": "tsx --test src/types/globe.test.ts src/types/itinerary.test.ts",
    "test:chunk:orchestrator-ui-slices": "tsx --test src/store/orchestrator-slice.test.ts src/store/ui-slice.test.ts",
    "postinstall": "rm -f public/cesium && ln -sf ../node_modules/cesium/Build/Cesium public/cesium",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "SYNTHETIC_BOTS_PROFILE=dev-lite npx tsx prisma/seed.ts",
    "db:seed:dev-lite": "SYNTHETIC_BOTS_PROFILE=dev-lite npx tsx prisma/seed.ts",
    "db:seed:demo-rich": "SYNTHETIC_BOTS_PROFILE=demo-rich npx tsx prisma/seed.ts",
    "db:seed:staging": "DOTENV_CONFIG_PATH=.env.staging npx tsx prisma/seed-staging.ts",
    "db:reset:staging": "./scripts/reset-staging.sh",
    "db:verify:staging": "DOTENV_CONFIG_PATH=.env.staging npx tsx scripts/verify-staging.ts"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.12",
    "@ai-sdk/react": "^3.0.40",
    "@auth/prisma-adapter": "^2.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@prisma/client": "^7.3.0",
    "@prisma/engines": "^7.3.0",
    "@prisma/extension-accelerate": "^3.0.1",
    "@reduxjs/toolkit": "^2.11.2",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.7.0",
    "ai": "^6.0.38",
    "bcryptjs": "^3.0.3",
    "cesium": "^1.137.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "hugeicons-react": "^0.4.0",
    "next": "16.1.2",
    "next-auth": "^5.0.0-beta.30",
    "prisma": "^7.3.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-globe.gl": "^2.37.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "resium": "^1.19.2",
    "stripe": "^20.3.0",
    "three": "^0.182.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "symlink-dir": "^9.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
